{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "lib": ["es2017"],
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "noUnusedLocals": true,
        "outDir": "./out"
    }
}
